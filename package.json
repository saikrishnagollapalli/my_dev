{
  "name": "nest-skeleton",
  "version": "0.0.1",
  "description": "Nest Js skeleton project which can be used as a starter app.",
  "author": "Aaron Rumao",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:performance": "artillery run %npm_config_path%",
    "test:performance:env": "artillery run --environment %npm_config_env% %npm_config_path%",
    "test:performance:quick": "artillery quick --count 20 --num 10 %npm_config_url%",
    "test:performance:report-generate": "artillery run --environment %npm_config_env% %npm_config_path% --output %npm_config_fileName%.json",
    "test:performance:report-view": "artillery report --output %npm_config_reportName%.html %npm_config_fileName%.json",
    "test:functional": "cucumber-js -p default",
    "test:functional:publish": "cucumber-js -p default --publish",
    "test:functional:quiet": "cucumber-js -p default --publish-quiet",
    "test:functional:tags": "cucumber-js -p default --tags \"%npm_config_tags%\"",
    "ci:audit": "npm audit --json > %npm_config_reportPath%.json",
    "ci:audit:report": "npm audit --omit=dev --json | npm-audit-html --output=./reports/audit.html",
    "ci:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --format json --output-file  %npm_config_reportPath%",
    "ci:retire": "retire --path . --outputformat json --outputpath %npm_config_reportPath% --exitwith 0"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.8",
    "@nestjs/cli": "^8.2.6",
    "@nestjs/common": "^8.4.6",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.4.6",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-fastify": "^8.4.6",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/terminus": "^8.0.6",
    "@nestjs/mongoose": "^9.2.0",
    "cache-manager": "^4.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "colors": "^1.4.0",
    "fastify-swagger": "^5.2.0",
    "jsonschema": "^1.4.1",
    "mssql": "^6.4.1",
    "mysql2": "^2.3.3",
    "oracledb": "^5.3.0",
    "passport-headerapikey": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "sqlite3": "^5.1.1",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^8.2.2",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.6",
    "@types/cache-manager": "^3.4.3",
    "@types/chai": "^4.3.1",
    "@types/jest": "^27.5.2",
    "@types/mssql": "^8.1.1",
    "@types/mysql": "^2.15.21",
    "@types/node": "^17.0.38",
    "@types/oracledb": "^5.2.3",
    "@types/sinon": "^10.0.11",
    "@types/sqlite3": "^3.1.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "mongodb-memory-server": "^8.8.0",
    "artillery": "^2.0.0-17",
    "artillery-plugin-expect": "^2.0.1",
    "chai": "^4.3.6",
    "cucumber-pretty": "^6.0.1",
    "cucumber-tsflow": "^4.0.0-preview.7",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "npm-audit-html": "^1.5.0",
    "npm-force-resolutions": "^0.0.10",
    "prettier": "^2.6.2",
    "retire": "^3.0.7",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-jest": "^27.1.5",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.2"
  },
  "resolutions": {
    "async": "3.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "^.*\\main\\.ts$"
    ],
    "testEnvironment": "node"
  }
}